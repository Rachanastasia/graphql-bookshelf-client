/ul>\n  )\n}\n\nexport default SubNav;\n\n","/home/rachel/MyProjects/my-first-graphql-client/src/hooks/useStarsForRating.js",["82"],"import React from 'react';\nimport { TiStar } from 'react-icons/ti';\n\nexport function useStarsForRating(num) {\n  switch (num) {\n    case 1:\n      return (\n        <div className='star-wrapper'>\n          <TiStar className='star-icon' />\n        </div>);\n\n    case 2:\n      return (\n        <div className='star-wrapper'>\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n        </div>);\n\n    case 3:\n      return (\n        <div className='star-wrapper'>\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n        </div>);\n\n    case 4:\n      return (\n        <div className='star-wrapper'>\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n        </div>);\n\n    case 5:\n      return (\n        <div className='star-wrapper'>\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n          <TiStar className='star-icon' />\n        </div>);\n\n  }\n}","/home/rachel/MyProjects/my-first-graphql-client/src/components/BookContent.js",[],"/home/rachel/MyProjects/my-first-graphql-client/src/hooks/hooks.js",[],["83","84"],"/home/rachel/MyProjects/my-first-graphql-client/src/components/Loading.js",[],"/home/rachel/MyProjects/my-first-graphql-client/src/components/SubNavItem.js",["85"],"/home/rachel/MyProjects/my-first-graphql-client/src/contexts/queries.js",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":2,"column":20,"nodeType":"92","messageId":"93","endLine":2,"endColumn":23},{"ruleId":"90","seve